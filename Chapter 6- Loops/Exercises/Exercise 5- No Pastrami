sandwich_orders=["Egg Sandwich","Pastrami Sandwich", "Grilled Cheese", "Pastrami Sandwich", "Spicy Chicken Sandwich", "Pastrami Sandwich", "Nutella Sandwich", "Grilled Chicken Sandwich"]
finished_sandwiches=[]

print("All out of pastrami today. Please try our other delicious items on the menu!")
while "Pastrami Sandwich" in sandwich_orders:
    sandwich_orders.remove("Pastrami Sandwich")

for sandwich in sandwich_orders:
    print("\nOrder for", sandwich,"received, preparation will now start.")
    finished_sandwiches.append(sandwich)

for sandwich in finished_sandwiches:
    print("\nOrder up for one", sandwich+"!")