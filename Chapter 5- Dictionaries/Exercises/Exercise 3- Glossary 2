glossary = {
    'String': 'A non-numerical value.',
    'Integer': 'A numerical value.',
    'List': 'A collection of values.',
    'Dictionary': 'A collection of pairs of values. One key value used to call out a specific value and the other value associated with the key value.',
    'Loop': 'Code that can run over and over again.',
    }


for a, b in glossary.items():
    print(a+ ":", b,"\n")